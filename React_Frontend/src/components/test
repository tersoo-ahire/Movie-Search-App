{movieData && movieData.Search && movieData.Search.length > 0 ? (
        <div>
          <h2>Search Results:</h2>
          {movieData.Search.map((result, index) => (
            <div key={index}>
              <h3>{result.Title}</h3>
              <p>Year: {result.Year}</p>
              {/* Add more movie information here */}
              <img
                src={result.Poster === "N/A" ? genericbanner : result.Poster}
                alt={`Poster for ${result.Title}`}
              />
            </div>
          ))}
        </div>
      ) : (
        <div></div>
      )}
    </div>

    <h2>Search Results</h2>
    <div className="sub-container">
      <p>
        Spiderman
        <span>
          Type: <span className="value"> Movie</span>
        </span>
        <span>
          Year:
          <span className="value"> 2023</span>
        </span>
        <span>
          IMBD Id:
          <span className="value"> tt8205250</span>
        </span>
      </p>
      <b>Total Results: 10</b>
    </div>
